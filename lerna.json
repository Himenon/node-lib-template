{
  "version": "0.0.2",
  "command": {
    "version": {
      "conventionalCommits": true,
      "changelogPreset": "angular-all",
      "exact": true
    }
  },
  "publish": {
    "access": "public",
    "allowBranch": "default",
    "ignoreChanges": [
      "CHANGELOG.md"
    ]
  },
  "stream": true,
  "concurrency": 1,
  "npmClient": "yarn",
  "packages": [
    "."
  ]
}
